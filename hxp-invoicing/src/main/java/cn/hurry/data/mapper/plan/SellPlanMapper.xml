<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.hurry.data.mapper.plan.SellPlanMapper">

	<update id="updateSellPlan" parameterType="byte">
		UPDATE sell_plan SET type=#{type}
	</update>

	<select id="selectSellPlan" resultType="byte">
		SELECT * FROM sell_plan limit 0,1
	</select>
</mapper> 